CREATE USER U1;
GRANT SELECT UPDATE
ON TABLE student,course
TO u1
WITH GRANT OPTION;

CREATE TRIGGER UP AFTER UPDATE
ON Course FOR EACH ROW
BEGIN
  UPDATE sc set sc.Cno=new.Cno where sc.Cno=old.Cno
END


SELECT course.Cno,Cname,count(Sno)
FROM course,sc
WHERE sc.Cno=course.Cno
GROUP BY course.Cno
HAVING count(Sno)>1
ORDER BY count(Sno) DESC,Cno ASC;
